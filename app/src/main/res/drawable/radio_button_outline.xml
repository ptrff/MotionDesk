<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <stroke android:color="?bottomNavBarChecked" android:width="2dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:color="?bottomNavBarNotChecked" android:width="2dp" />
        </shape>
    </item>
</selector>